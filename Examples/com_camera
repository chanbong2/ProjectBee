// camera

#include <HardwareSerial.h>
HardwareSerial MySerial(0);

void setup() {
  // put your setup code here, to run once:
  // Serial.begin(115200);
  MySerial.begin(9600, SERIAL_8N1, -1, -1);
}

int counter = 0;

void loop() {
  // put your main code here, to run repeatedly:
  MySerial.println(counter);
  counter++;
  delay(2000);
}
